{
    "name": "litsea/litsea",
    "type": "library",
    "description": "Litsea PHP framework",
    "keywords": ["framework", "litsea", "php"],
    "minimum-stability": "dev",
    "license": "WTFPL",
    "authors": [
        {
            "name": "lostsnow",
            "email": "lostsnow@gmail.com",
            "homepage": "http://www.lsproc.com/"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "ext-curl": "required by Http",
        "ext-pdo": "required by ORM",
        "ext-pdo_mysql": "required by ORM"
    },
    "autoload": {
        "psr-4": {
            "Litsea\\": "src/Litsea"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Litsea\\Test\\": "tests/Litsea"
        }
    }
}